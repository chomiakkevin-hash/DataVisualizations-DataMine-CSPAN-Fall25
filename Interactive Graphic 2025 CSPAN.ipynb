  {
    "# ----  Settings  ----\n",
    "file_path <- \"/anvil/projects/x-cis220051/corporate/ccspan-congressional/Vizualization/Video data/video_training_log.csv\" \n",
    "sheet_name <- \"Sheet1\"                 \n",
    "\n",
    "\n",
    "# ---- Packages ----\n",
    "req <- c(\"readxl\", \"dplyr\", \"tidyr\", \"plotly\", \"htmlwidgets\", \"tools\", \"stringr\")\n",
    "new <- setdiff(req, rownames(installed.packages()))\n",
    "if (length(new)) install.packages(new, dependencies = TRUE)\n",
    "lapply(req, library, character.only = TRUE) |> invisible()\n",
    "\n",
    "# ---- Read data ----\n",
    "ext <- tolower(tools::file_ext(file_path))\n",
    "if (ext %in% c(\"xlsx\",\"xls\")) {\n",
    "  df <- readxl::read_excel(file_path, sheet = sheet_name)\n",
    "} else if (ext %in% c(\"csv\",\"txt\")) {\n",
    "  df <- read.csv(file_path, header = TRUE, check.names = FALSE)\n",
    "} else {\n",
    "  stop(\"Unsupported file type. Please use .xlsx/.xls or .csv\")\n",
    "}\n",
    "\n",
    "# ---- Keep original ----\n",
    "df_original <- df\n",
    "\n",
    "# ---- Normalize column  ----\n",
    "norm_names <- function(x) {\n",
    "  x |>\n",
    "    tolower() |>\n",
    "    stringr::str_trim() |>\n",
    "    (\\(s) gsub(\"[\\\\s\\\\-]+\", \"_\", s))() |>\n",
    "    (\\(s) gsub(\"[^a-z0-9_]\", \"\", s))()\n",
    "}\n",
    "cn <- norm_names(colnames(df))\n",
    "\n",
    "# ---- Locate epoch ----\n",
    "epoch_candidates <- c(\"epoch\",\"step\",\"iteration\",\"iter\",\"batch\",\"round\")\n",
    "epoch_idx <- match(epoch_candidates, cn)\n",
    "epoch_idx <- epoch_idx[!is.na(epoch_idx)]\n",
    "if (length(epoch_idx) >= 1) {\n",
    "  epoch_col <- colnames(df)[epoch_idx[1]]\n",
    "} else {\n",
    "# ---- Create an epoch column at the front ----\n",
    "  df$epoch <- seq_len(nrow(df))\n",
    "  epoch_col <- \"epoch\"\n",
    "  cn <- c(\"epoch\", cn)  \n",
    "}\n",
    "\n",
    "# ---- Detect dataset and metric ----\n",
    "detect_parts <- function(name_norm) {\n",
    "# ---- Dataset ----\n",
    "  if (grepl(\"\\\\b(val|valid|validation)\\\\b\", name_norm)) ds <- \"validation\"\n",
    "  else if (startsWith(name_norm, \"val\"))                ds <- \"validation\"\n",
    "  else ds <- NA_character_\n",
    "# ---- Metric ----\n",
    "  if (grepl(\"\\\\bloss\\\\b\", name_norm))                   mt <- \"loss\"\n",
    "  else if (grepl(\"\\\\bacc\\\\b|\\\\baccuracy\\\\b\", name_norm)) mt <- \"accuracy\"\n",
    "  else mt <- NA_character_\n",
    "  list(dataset = ds, metric = mt)\n",
    "}\n",
    "\n",
    "# ---- Build long frame ----\n",
    "epoch_idx_actual <- which(colnames(df) == epoch_col)\n",
    "other_idx <- setdiff(seq_along(df), epoch_idx_actual)\n",
    "long_list <- list()\n",
    "\n",
    "for (i in other_idx) {\n",
    "  nm_raw <- colnames(df)[i]\n",
    "  nm_norm <- norm_names(nm_raw)\n",
    "  parts <- detect_parts(nm_norm)\n",
    "  ds <- parts$dataset\n",
    "  mt <- parts$metric\n",
    "  if (!is.na(ds) && !is.na(mt) && is.numeric(df[[i]])) {\n",
    "    long_list[[length(long_list)+1]] <- data.frame(\n",
    "      epoch   = df[[epoch_col]],\n",
    "      dataset = ds,\n",
    "      metric  = mt,\n",
    "      value   = as.numeric(df[[i]]),\n",
    "      stringsAsFactors = FALSE\n",
    "    )\n",
    "  }\n",
    "}\n",
    "# ---- If nothing matches, plot all numeric columns as a separate metric ----\n",
    "if (length(long_list) == 0) {\n",
    "  numeric_cols <- which(sapply(df[other_idx], is.numeric))\n",
    "  if (length(numeric_cols) == 0) stop(\"No numeric columns found to plot.\")\n",
    "  for (j in other_idx[numeric_cols]) {\n",
    "    nm <- colnames(df)[j]\n",
    "    long_list[[length(long_list)+1]] <- data.frame(\n",
    "      epoch   = df[[epoch_col]],\n",
    "      dataset = \"series\",\n",
    "      metric  = nm,\n",
    "      value   = as.numeric(df[[j]]),\n",
    "      stringsAsFactors = FALSE\n",
    "    )\n",
    "  }\n",
    "}\n",
    "\n",
    "long <- dplyr::bind_rows(long_list) |>\n",
    "  dplyr::arrange(metric, dataset, epoch)\n",
    "\n",
    "# ---- Build figure ----\n",
    "dash_map <- c(training = \"solid\", validation = \"dash\")\n",
    "metrics  <- unique(long$metric)\n",
    "datasets <- unique(long$dataset)\n",
    "\n",
    "p <- plotly::plot_ly()\n",
    "for (m in metrics) {\n",
    "  for (d in datasets) {\n",
    "    sub <- dplyr::filter(long, metric == m, dataset == d)\n",
    "    if (nrow(sub)) {\n",
    "      p <- p |>\n",
    "        plotly::add_trace(\n",
    "          x = sub$epoch, y = sub$value,\n",
    "          type = \"scatter\", mode = \"lines+markers\",\n",
    "          name = paste0(m, \" (\", d, \")\"),\n",
    "          line = list(dash = ifelse(d %in% names(dash_map), dash_map[[d]], \"dot\")),\n",
    "          hovertemplate = paste(\n",
    "            \"Epoch: %{x}<br>\",\n",
    "            \"Metric: \", m, \"<br>\",\n",
    "            \"Dataset: \", d, \"<br>\",\n",
    "            \"Value: %{y:.4f}<extra></extra>\"\n",
    "          )\n",
    "        )\n",
    "    }\n",
    "  }\n",
    "}\n",
    "\n",
    "p <- p |>\n",
    "  plotly::layout(\n",
    "    title  = list(text = \"Video Metrics by Epoch\", x = 0),\n",
    "    xaxis  = list(title = \"Epoch\"),\n",
    "    yaxis  = list(title = \"Training Loss Value\"),\n",
    "    legend = list(orientation = \"h\", x = 0, y = -0.2),\n",
    "    hovermode = \"x unified\"\n",
    "  ) |>\n",
    "  plotly::config(displaylogo = FALSE)  # ---- clean toolbar ----\n",
    "\n",
    "p\n",
    "\n",
    "# ---- HTML ----\n",
    "out_html <- \"interactive_results.html\"\n",
    "htmlwidgets::saveWidget(p, file = out_html, selfcontained = TRUE)\n",
    "\n",
    "# --- Embed ---\n",
    "cat('\n",
    "Embed:\n",
    "<iframe src=\"/path/to/interactive_results.html\" width=\"100%\" height=\"520\" style=\"border:none;\"></iframe>\n",
    "\\n')\n"
   ]
  }
 ],
