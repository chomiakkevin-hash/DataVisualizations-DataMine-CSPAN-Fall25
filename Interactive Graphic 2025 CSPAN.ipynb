   "source": [
    "# ---- Libraries ---- \n",
    "suppressPackageStartupMessages({\n",
    "  library(dplyr)\n",
    "  library(ggplot2)\n",
    "  library(plotly)\n",
    "})\n",
    "\n",
    "video_predictions <- suppressMessages(\n",
    "  read.csv(\"/anvil/projects/x-cis220051/corporate/ccspan-congressional/Vizualization/Video data/video_predictions.csv\", stringsAsFactors = FALSE)\n",
    ")\n",
    "\n",
    "# ---- Packages ----\n",
    "req <- c(\"dplyr\",\"tidyr\",\"plotly\",\"stringr\",\"readr\",\"tools\")\n",
    "new <- setdiff(req, rownames(installed.packages()))\n",
    "if (length(new)) install.packages(new, dependencies = TRUE)\n",
    "invisible(lapply(req, library, character.only = TRUE))\n",
    "\n",
    "# ---- Helpers ----\n",
    "norm_names <- function(x) {\n",
    "  x |>\n",
    "    tolower() |>\n",
    "    stringr::str_trim() |>\n",
    "    (\\(s) gsub(\"[\\\\s\\\\-]+\", \"_\", s))() |>\n",
    "    (\\(s) gsub(\"[^a-z0-9_]\", \"\", s))()\n",
    "}\n",
    "safe_ascii <- function(s) iconv(s, from = \"\", to = \"ASCII//TRANSLIT\")\n",
    "\n",
    "# ---- Locate and load CSV ----\n",
    "candidate_paths <- c(\n",
    "  \"/anvil/projects/x-cis220051/corporate/ccspan-congressional/Vizualization/Video data/video_predictions.csv\",\n",
    "  \"/anvil/projects/x-cis220051/corporate/ccspan-congressional/Vizualization/video_predictions.csv\",\n",
    "  \"/home/x-kchomiak/Project 4/cspan/video_predictions.csv\",\n",
    "  \"video_predictions.csv\",\n",
    "  \"./video_predictions.csv\",\n",
    "  \"./data/video_predictions.csv\",\n",
    "  \"../video_predictions.csv\"\n",
    ")\n",
    "file_path <- NA_character_\n",
    "for (p in candidate_paths) if (file.exists(p)) { file_path <- p; break }\n",
    "if (is.na(file_path)) {\n",
    "  message(\"Could not find the CSV in common locations. Opening a file pickerâ€¦\")\n",
    "  file_path <- tryCatch(file.choose(), error = function(e) NA_character_)\n",
    "}\n",
    "stopifnot(!is.na(file_path) && file.exists(file_path))\n",
    "\n",
    "df <- readr::read_csv(file_path, show_col_types = FALSE, progress = FALSE)\n",
    "\n",
    "# ---- Normalize names ----\n",
    "cn_norm <- norm_names(colnames(df))\n",
    "\n",
    "# ---- Choose x-axis ----\n",
    "x_candidates <- c(\"time\",\"timestamp\",\"frame\",\"step\",\"iter\",\"iteration\",\"epoch\",\"index\")\n",
    "pick_idx <- match(x_candidates, cn_norm)\n",
    "pick_idx <- pick_idx[!is.na(pick_idx)]\n",
    "if (length(pick_idx)) {\n",
    "  x_col <- colnames(df)[pick_idx[1]]\n",
    "} else {\n",
    "  x_col <- \"._row_index\"\n",
    "  df[[x_col]] <- seq_len(nrow(df))\n",
    "}\n",
    "\n",
    "# Refresh normalized names \n",
    "cn_norm <- norm_names(colnames(df))\n",
    "\n",
    "# ---- Parse time ----\n",
    "if (is.character(df[[x_col]])) {\n",
    "  try({\n",
    "    parsed <- suppressWarnings(as.POSIXct(df[[x_col]]))\n",
    "    if (all(!is.na(parsed))) df[[x_col]] <- parsed\n",
    "  }, silent = TRUE)\n",
    "}\n",
    "\n",
    "# ---- Identify video columns and ensure all entries are numeric ----\n",
    "is_num <- sapply(df, is.numeric)\n",
    "audio_like <- grepl(\"audio|sound\", cn_norm)\n",
    "mask <- is_num & audio_like & (colnames(df) != x_col)\n",
    "audio_cols <- colnames(df)[mask]\n",
    "\n",
    "if (length(audio_cols) == 0) {\n",
    "  audio_cols <- setdiff(colnames(df)[is_num], x_col)\n",
    "}\n",
    "if (length(audio_cols) == 0) {\n",
    "  audio_cols <- \"._value\"\n",
    "  df[[audio_cols]] <- seq_len(nrow(df))\n",
    "}\n",
    "\n",
    "# ---- Reformat data for Plotting ----\n",
    "long <- df[, c(x_col, audio_cols), drop = FALSE] |>\n",
    "  tidyr::pivot_longer(cols = -dplyr::all_of(x_col),\n",
    "                      names_to = \"series\", values_to = \"value\")\n",
    "\n",
    "# ---- Build dropdown ----\n",
    "buttons <- list()\n",
    "p <- plotly::plot_ly()\n",
    "for (i in seq_along(audio_cols)) {\n",
    "  col_i <- audio_cols[i]\n",
    "  sub <- dplyr::filter(long, series == col_i)\n",
    "  p <- p |>\n",
    "    plotly::add_trace(\n",
    "      data = sub,\n",
    "      x = ~ .data[[x_col]],\n",
    "      y = ~ value,\n",
    "      type = \"scatter\",\n",
    "      mode = \"lines\",\n",
    "      name = col_i,\n",
    "      visible = ifelse(i == 1, TRUE, FALSE),\n",
    "      line = list(width = 2, shape = \"spline\")\n",
    "    )\n",
    "  vis <- rep(FALSE, length(audio_cols)); vis[i] <- TRUE\n",
    "    \n",
    "  # ---- ASCII to prevent U+2013 showing up ----\n",
    "  ttl <- safe_ascii(paste(\"Audio Value Over Time -\", col_i))\n",
    "  buttons[[i]] <- list(\n",
    "    method = \"update\",\n",
    "    args = list(list(visible = vis),\n",
    "                list(title = ttl)),\n",
    "    label = col_i\n",
    "  )\n",
    "}\n",
    "\n",
    "initial_title <- safe_ascii(paste(\"Video Value Over Time -\", audio_cols[1]))\n",
    "\n",
    "p <- p |>\n",
    "  plotly::layout(\n",
    "    title  = initial_title,\n",
    "    xaxis  = list(title = \"Index Value\"),\n",
    "    yaxis  = list(title = \"Video Value\"),\n",
    "    updatemenus = list(list(\n",
    "      active = 0,\n",
    "      buttons = buttons,\n",
    "      x = 1.02, y = 1, xanchor = \"left\"\n",
    "    )),\n",
    "    hovermode = \"x unified\"\n",
    "  ) |>\n",
    "  plotly::config(displaylogo = FALSE)\n",
    "\n",
    "p\n"
   ]
